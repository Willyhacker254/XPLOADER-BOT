{
  "name": "XploaderBot",
  "version": "3.2.6",
  "description": "Xploader, A modular advanced whatsapp bot",
  "logo": "https://i.ibb.co/0tbxXc0/Xploader5.jpg",
  "keywords": ["whatsapp", "bot", "md", "multi-device", "Xploader", "V3", "wabot", "termux-bot", "sc-bot"],
  "repository": "https://github.com/Dark-Xploit/XPLOADER-BOT",
  "stack": "container",
   "formation": {
    "web": {
      "quantity": 1,
      "size": "Standard-2X"
    }
  },
  "env": {
    "OWNER_NUMBER": {
      "description": "Enter your number here",
      "required": true,
      "value": "254796180105"
    },
    "OWNER_NAME": {
      "description": "Enter your name here",
      "required": true,
      "value": "Tylor"
     },
    "WELCOME_MSG": {
      "description": "Set true to enable welcome and left messages in groups and false to disable",
      "required": false,
      "value": "false"
     },
    "AUTO_STATUS_SEEN": {
      "description": "Set true to enable and false to disable auto status view",
      "required": false,
      "value": "true"
     },
    "AUTO_STATUS_REACT": {
      "description": "Set true to enable and false to disable auto status reaction",
      "required": false,
      "value": "true"
     },
    "STATUS_EMOJI": {
      "description": "Set the emoji that you want to be reacted to status",
      "required": false,
      "value": "🧡"
    },
    "PREFIX": {
      "description": "Enter your desired prefix here",
      "required": true,
      "value": "."
    },
    "BOT_NAME": {
      "description": "Enter Bot name here",
      "required": false,
      "value": "𝐗𝐩𝐥𝐨𝐚𝐝𝐞𝐫𝐁𝐨𝐭"
    },
    "SESSION_ID": {
      "description": "Enter Xploader session id here; must start with XPLOADER~",
      "required": true,
      "value": ""
    },
    "TIMEZONE": {
      "description": "Enter your timezone here. Google or don't edit if you don't know!",
      "required": false,
      "value": "Africa/Nairobi"
    },
     "STICKER_PACK_NAME": {
      "description": "Enter Sticker pack name here",
      "required": false,
      "value": "Xploader"
     },
     "STICKER_AUTHOR_NAME": {
      "description": "Enter sticker author name here",
      "required": false,
      "value": "Bot"
     },
     "PREMIUM_VIP": {
      "description": "Enter numbers to be premium users [ Each Saperated By Comma ]",
      "required": false,
      "value": "254754783972,254796180105"
     },
    "ANTI_CALL": {
      "description": "Set true to enable and false to disable bot blocking user's who call you",
      "required": false,
      "value": "false"
     },
     "AUTO_BLOCK": {
      "description": "Enter a country code to be auto blocked",
      "required": false,
      "value": "988"
     }
    },
    "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "https://github.com/DuckyTeam/heroku-buildpack-imagemagick.git"
    },
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
    },
    {
      "url": "https://github.com/clhuang/heroku-buildpack-webp-binaries.git"
    }
  ]
}

